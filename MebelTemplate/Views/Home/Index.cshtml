@model MebelTemplate.Models.HomeVM.HomeVM

<!-- slider-start -->
<div class="slider-container">
    <div class="slider">
        <!-- Slider Image -->
        <div id="mainslider" class="nivoSlider slider-image">
            @for (int i = 0; i < Model.GetHomes.Count(); i++)
            {
                <img style="height:430px;" class="img-responsive" src="~/Content/img/slider/@(Model.GetHomes.ElementAt(i).Id)/@(Model.GetHomes.ElementAt(i).Image)" title="#htmlcaption@(Model.GetHomes.ElementAt(i).Id)" />
            }
        </div>
        <!-- Slider Caption 1 -->
        @for (int i = 0; i < Model.GetHomes.Count(); i++)
        {
            <div id="htmlcaption@(Model.GetHomes.ElementAt(i).Id)" class="nivo-html-caption slider-caption-@(Model.GetHomes.ElementAt(i).Id)">
                <div class="slider-progress"></div>
                <div class="slide@(Model.GetHomes.ElementAt(i).Id)-text slide-@Model.GetHomes.ElementAt(i).Id">
                    <div class="middle-text">
                        <div class="cap-dec wow bounceInLeft" data-wow-duration="0.9s" data-wow-delay="0s">
                            <h1>@Model.GetHomes.ElementAt(i).HeaderTitle</h1>
                        </div>
                        <div class="cap-title wow bounceInRight" data-wow-duration="1.2s" data-wow-delay="0.2s">
                            <h3>From: @Model.GetHomes.ElementAt(i).HeaderPrice</h3>
                        </div>
                        <div class="cap-readmore wow bounceInUp" data-wow-duration="1.3s" data-wow-delay=".5s">
                            <a href="#">Shop Now</a>
                        </div>
                    </div>
                </div>
            </div>
        }

    </div>
</div>
<!-- slider-end -->
<!-- service-start -->
<div class="service-area">
    <div class="container">
        <div class="row">
            @foreach (var item in Model.GetServices)
            {
                <div class="col-md-4 col-sm-4 service">
                    <div class="service-logo">
                        <img src="~/Content/img/service/@item.Id/@item.Image" />
                    </div>
                    <div class="service-info">
                        <h2>@item.ServicesTitle</h2>
                        <p>@item.ServicesContent</p>
                    </div>
                </div>

            }
        </div>
    </div>
</div>
<!-- service-end -->
<!-- banner-start -->
<div class="banner-area">
    <div class="container">
        <div class="row">
            @for (int i = 0; i < Model.GetBanners.Take(2).OrderBy(x=>x.Id).Count(); i++)
            {
                <div class="col-md-6 col-sm-6 col-xs-12 @(i==1?"banner-area-box":"") ">
                    <div class="banner-box">
                        @if (i == 1)
                        {
                            <div class="banner-content">
                                <h2>@(Model.GetBanners.ElementAt(i).Id).</h2>
                                <h3>@(Model.GetBanners.ElementAt(i).Category.CatName)</h3>
                                <p>
                                    @(Model.GetBanners.ElementAt(i).BannerContent)                                 
                                </p>
                                <a href="#">Shop now</a>
                            </div>
                            <div class="banner-picture">
                                <a href="#">
                                    <img src="~/Content/img/banner/@(Model.GetBanners.ElementAt(i).Id)/@(Model.GetBanners.ElementAt(i).Image)"/>
                                </a>
                            </div>
                        }
                        else
                        {
                            <div class="banner-picture">
                                <a href="#">
                                    <img src="~/Content/img/banner/@(Model.GetBanners.ElementAt(i).Id)/@(Model.GetBanners.ElementAt(i).Image)"/>
                                </a>
                            </div>
                            <div class="banner-content">
                                <h2>@(Model.GetBanners.ElementAt(i).Id).</h2>
                                <h3>@(Model.GetBanners.ElementAt(i).Category.CatName)</h3>
                                <p>
                                    @(Model.GetBanners.ElementAt(i).BannerContent)
                                </p>
                                <a href="#">Shop now</a>
                            </div>
                        }
                    </div>
                </div>
            }

        </div>
        <div class="row banner-marg">
            @foreach (var item in Model.GetBanners.Skip(2).Take(3).OrderBy(x=>x.Id))
            {
                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="banner-static-box no-mar">
                        <div class="banner-box">
                            <a href="#">
                                <img src="~/Content/img/banner/@(item.Id)/@item.Image" alt="" />
                            </a>
                        </div>
                        <div class="banner-box-content">
                            <h2>@(item.Id).</h2>
                            <h4>@(item.Category.CatName)</h4>
                            <a href="#">Shop now</a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<!-- banner-end -->
